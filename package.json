{
  "name": "@apexxcloud/sdk-node",
  "version": "1.0.13",
  "description": "Official Node.js SDK for ApexxCloud Storage Service",
  "main": "index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./src/sdk.js",
      "import": "./src/sdk.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "prepare": "husky install",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "format": "prettier --write \"src/**/*.{js,ts}\""
  },
  "keywords": [
    "apexxcloud",
    "storage",
    "sdk",
    "cloud-storage",
    "file-upload"
  ],
  "author": "ApexxCloud <team@apexxcloud.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apexxcloud/apexxcloud-sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/apexxcloud/apexxcloud-sdk-node/issues"
  },
  "homepage": "https://github.com/apexxcloud/apexxcloud-sdk-node#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
